{
    "compilerOptions": {
        "strict": true,
        "exactOptionalPropertyTypes": true,
        "moduleDetection": "force",
        "composite": true,
        "downlevelIteration": true,
        "resolveJsonModule": true,
        "esModuleInterop": false,
        "declaration": true,
        "skipLibCheck": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "moduleResolution": "NodeNext",
        "lib": ["ES2022"],
        "types": [],
        "isolatedModules": true,
        "sourceMap": true,
        "declarationMap": true,
        "noImplicitReturns": false,
        "noUnusedLocals": true,
        "noUnusedParameters": false,
        "noFallthroughCasesInSwitch": true,
        "noEmitOnError": false,
        "noErrorTruncation": false,
        "allowJs": false,
        "checkJs": false,
        "forceConsistentCasingInFileNames": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "noUncheckedIndexedAccess": false,
        "strictNullChecks": true,
        "baseUrl": ".",
        "target": "ES2022",
        "module": "NodeNext",
        "incremental": true,
        "removeComments": false,
        "plugins": [{ "name": "@effect/language-service" }],
        "paths": {
            "@shocae/apk-downloader": ["./packages/apk-downloader/src/index.js"],
            "@shocae/apk-downloader/*": ["./packages/apk-downloader/src/*.js"],
            "@shocae/emulator": ["./packages/emulator/src/index.js"],
            "@shocae/emulator/*": ["./packages/emulator/src/*.js"],
            "@shocae/control": ["./packages/control/src/index.js"],
            "@shocae/control/*": ["./packages/control/src/*.js"]
        }
    }
}
